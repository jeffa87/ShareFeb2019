pipeline {
    agent {
        label 'dcdLinux1'
    }

    environment{
        ALLJE05_MF_PASSWORD = credentials('${credentialID}')
    }
    
    stages {

        stage('List User'){
            steps{
                sh "./listJenkins.sh ${system} ${id} ${ALLJE05_MF_PASSWORD} ${listID}"
            }
        }
        
    }
    post {
        always {
            echo "Post Always start"
                cleanWs()
        }
    }
}
