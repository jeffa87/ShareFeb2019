pipeline {
    agent {
        label 'dcdLinux1'
    }
    
    stages {

        stage('List User'){
            steps{
                sh "./listJenkins.sh ${system} ${id} ${password} ${listID}"
            }
        }
        
    }
    post {
        always {
            echo "Post Always start"
                cleanWs()
        }
    }
}
