pipeline {
    agent {
        label 'dcdLinux1'
    }
    
    stages {

        stage('List User'){
            steps{
                withCredentials([string(credentialsId: 'password', variable: 'password')]) {
                    sh "./listJenkins.sh ${system} ${id} ${password} ${listID}"
                }
            }
        }
        
    }
    post {
        always {
            echo "Post Always start"
                cleanWs()
        }
    }
}
